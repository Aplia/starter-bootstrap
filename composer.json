{
    "name": "aplia/starter-bootstrap",
    "description": "Common code for bootstrapping eZ publish projects",
    "homepage": "https://github.com/Aplia/starter-bootstrap",
    "license": "MIT",
    "authors": [
        {
            "name": "Aplia dev-team",
            "homepage": "https://github.com/Aplia"
        }
    ],
    "require": {
        "php": ">=5.3",
        "composer/composer": ">=1.0",
        "aplia/support": "^1.0.0",
        "filp/whoops": "2.1.14|1.1.10",
        "vlucas/phpdotenv": "2.0.1|2.3.0",
        "nikic/php-parser": "^2.1.1|^1.4.1|~3.0",
        "monolog/monolog": "1.21.0"
    },
    "suggest": {
        "sentry/sentry": "Package for integrating with Sentry error service"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.4"
    },
    "bin": [
        "bin/ezp_dump_extensions",
        "bin/bootstrap_build",
        "bin/bootstrap_clean",
        "bin/bootstrap_config",
        "bin/bootstrap_describe",
        "bin/bootstrap_test_sentry"
    ],
    "autoload": {
        "psr-4": {
            "Aplia\\": "Aplia"
        }
    }
}
