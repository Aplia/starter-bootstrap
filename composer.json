{
    "name": "aplia/starter-bootstrap",
    "description": "Common code for bootstrapping Starter projects for use with eZ publish",
    "homepage": "https://bitbucket.org/aplia/starter-bootstrap",
    "license": "MIT",
    "authors": [
        {
            "name": "Aplia dev-team",
            "homepage": "https://bitbucket.org/aplia"
        }
    ],
    "require": {
        "php": ">=5.3",
        "composer/composer": ">=1.0",
        "aplia/support": "^1.0.0",
        "filp/whoops": "1.1.10",
        "vlucas/phpdotenv": "2.0.1|2.3.0",
        "nikic/php-parser": "^2.1.1",
        "sentry/sentry": "1.5.0",
        "monolog/monolog": "1.21.0"
    },
    "bin": [
        "bin/ezp_dump_extensions",
        "bin/bootstrap_build",
        "bin/bootstrap_clean",
        "bin/bootstrap_config"
    ],
    "autoload": {
        "psr-4": {
            "Aplia\\": "Aplia"
        }
    }
}
